{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/chartRoom/index",
			"style": {
				"navigationBarTitleText": "聊天室",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "主页"
			}
		},
		{
			"path": "pages/my/index",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}
	],
	// tabbar配置
	"tabBar": {
			"color": "#999999",
			"selectedColor": "#4b93ff",
			"borderStyle": "white",
			"backgroundColor": "#F7F7F7",
			"fontSize": "16px",
			"iconWidth": "30px",
			"list": [{
				"pagePath": "pages/chartRoom/index",
				"iconPath": "static/images/tabbar/chart.png",
				"selectedIconPath": "static/images/tabbar/chart-active.png",
				"text": "聊天"
			},{
				"pagePath": "pages/index/index",
				"iconPath": "static/images/tabbar/home.png",
				"selectedIconPath": "static/images/tabbar/home-active.png",
				"text": "主页"
			},
			{
				"pagePath": "pages/my/index",
				"iconPath": "static/images/tabbar/my.png",
				"selectedIconPath": "static/images/tabbar/my-active.png",
				"text": "我的"
			}]
	},
	"globalStyle": {
		"navigationStyle": "default",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "数智名片",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}
